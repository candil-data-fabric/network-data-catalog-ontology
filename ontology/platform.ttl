#################################
#    Object Properties
#################################

## ndc-pm:platform
ndc-pm:platform rdf:type owl:ObjectProperty ;
		rdfs:domain ndc-sr:NetworkDevice ;
		rdfs:range ndc-pm:Platform ;
		rdfs:label "platform" .

## ndc-pm:operatingSystem
ndc-pm:operatingSystem rdf:type owl:ObjectProperty ;
		rdfs:domain ndc-sr:NetworkDevice ;
		rdfs:range ndc-pm:OperatingSystem ;
		rdfs:label "operating system" .

## ndc-pm:vendor
ndc-pm:vendor rdf:type owl:ObjectProperty ;
		rdfs:range ndc-pm:Vendor ;
		rdfs:label "vendor" .

## ndc-pm:vendor
ndc-pm:vendor rdf:type owl:ObjectProperty ;
		rdfs:range ndc-pm:Vendor ;
		rdfs:label "vendor" .

#################################
#    Data Properties
#################################

## ndc-pm:platformName
ndc-pm:platformName rdf:type owl:DatatypeProperty ;
		rdfs:domain ndc-pm:Platform ;
		rdfs:range xsd:string ;
		rdfs:label "platform name" .

## ndc-pm:osName
ndc-pm:osName rdf:type owl:DatatypeProperty ;
		rdfs:domain ndc-pm:OperatingSystem ;
		rdfs:range xsd:string ;
		rdfs:label "os name" .

## ndc-pm:osVersion
ndc-pm:osVersion rdf:type owl:DatatypeProperty ;
		rdfs:domain ndc-pm:OperatingSystem ;
		rdfs:range xsd:string ;
		rdfs:label "os version" .

## schema:name
schema:name rdf:type owl:DatatypeProperty ;
		rdfs:range xsd:string ;
		rdfs:label "name" .

#################################
#    Classes
#################################

## ndc-pm:Platform
ndc-pm:Platform rdf:type owl:Class ;
	rdfs:label "Platform" .

## ndc-pm:Vendor
ndc-pm:Vendor rdf:type owl:Class ;
	rdfs:label "Vendor" ;
	rdfs:subClassOf schema:Organization .

## ndc-pm:OperatingSystem
ndc-pm:OperatingSystem rdf:type owl:Class ;
	rdfs:label "Operating System" .

## ndc-sr:NetworkDevice
ndc-sr:NetworkDevice rdf:type owl:Class ;
	rdfs:label "Network Device" .

## schema:Organization
schema:Organization rdf:type owl:Class ;
	rdfs:label "Organization" .

